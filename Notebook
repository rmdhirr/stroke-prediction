        "# Calculate the absolute correlations of all variables with 'stroke'\n",
        "corr_with_target = df[numerical_cols].corr()['stroke'].abs()\n",
        "\n",
        "# Sort the correlations in descending order\n",
        "sorted_corr_with_target = corr_with_target.sort_values(ascending=False)\n",
        "\n",
        "# Remove the self-correlation of 'stroke' with itself\n",
        "sorted_corr_with_target = sorted_corr_with_target[sorted_corr_with_target.index != 'stroke']\n",
        "\n",
        "# Convert the Series into a DataFrame\n",
        "df_corr_with_target = pd.DataFrame(sorted_corr_with_target)\n",
        "\n",
        "# Visualize the correlations as a heatmap\n",
        "plt.figure(figsize=(1, len(df_corr_with_target)))\n",
        "sns.heatmap(df_corr_with_target, annot=True, cmap='coolwarm', cbar=False)\n",
        "plt.title('Correlation Heatmap of Numerical Variables with Target Variable \"Stroke\"')\n",
        "plt.show()"
      ]
