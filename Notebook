      "source": [
        "# Training function for Hyperparameter Tuned Decision Tree\n",
        "def train_decision_tree_ht(X_train, y_train):\n",
        "    \"\"\"Trains a hyperparameter-tuned Decision Tree model.\"\"\"\n",
        "    rcv_dt_ht.fit(X_train, y_train)\n",
        "    return rcv_dt_ht"
      ]
