        "def visualize_classification_report_rf_ht(rf_ht, X_test, y_test):\n",
        "    \"\"\"Visualizes the classification report for the hyperparameter-tuned Random Forest model.\"\"\"\n",
        "    y_pred_rf = rf_ht.predict(X_test)\n",
        "    print(\"Random Forest (Hyperparameter-Tuned) Classification Report:\")\n",
        "    print(classification_report(y_test, y_pred_rf))\n",
        "    report_rf = classification_report(y_test, y_pred_rf, output_dict=True)\n",
        "    df_rf = pd.DataFrame(report_rf).transpose()\n",
        "    metrics_df = df_rf[['precision', 'recall', 'f1-score']].drop(index=['accuracy', 'support'], errors='ignore')\n",
        "    ax = metrics_df.plot(kind='bar', figsize=(10, 6))\n",
        "    plt.title(\"Random Forest (Hyperparameter-Tuned) Classification Report (Metrics)\")\n",
        "    plt.xlabel(\"Metrics and Classes\")\n",
        "    plt.ylabel(\"Score\")\n",
        "    plt.xticks(rotation=0)\n",
        "    max_height = metrics_df.max().max()\n",
        "    ax.set_ylim(0, max_height + 0.1 * max_height)\n",
        "    for p in ax.patches:\n",
        "        ax.annotate(format(p.get_height(), '.2f'),\n",
        "                    (p.get_x() + p.get_width() / 2., p.get_height()),\n",
        "                    ha='center', va='bottom', xytext=(0, 10), textcoords='offset points')\n",
        "    plt.tight_layout()\n",
        "    plt.show()"
      ]
